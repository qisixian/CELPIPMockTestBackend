<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.CELPIPMockTest.CELPIPMockTestBackend.common.MediaFilesMapper">
    <select id="getMediaFileById" resultMap="MediaFilesMap">
        select * from media_files where id = #{id}
    </select>
    <resultMap id="MediaFilesMap" type="com.CELPIPMockTest.CELPIPMockTestBackend.common.MediaFiles">
        <id property="id" column="id" />
        <result property="name" column="name" />
        <result property="data" column="data" />
    </resultMap>
</mapper>